# Test Environment Configuration
# Copy this file to .env.test and customize as needed

# AWS Testing Configuration
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_SESSION_TOKEN=test

# Test Account Configuration
TEST_ADMIN_ACCOUNT=123456789012
TEST_SECURITY_ACCOUNT=234567890123
TEST_ORG_ID=o-example12345
TEST_ROOT_OU=r-example12345
TEST_CROSS_ACCOUNT_ROLE=AWSControlTowerExecution

# Test Regions (comma-separated)
TEST_REGIONS=us-east-1,us-west-2,eu-west-1

# Test Execution Control
RUN_INTEGRATION_TESTS=true
SKIP_SLOW_TESTS=false

# Service-Specific Test Configuration
TEST_AWS_CONFIG_ENABLED=Yes
TEST_GUARDDUTY_ENABLED=Yes
TEST_SECURITY_HUB_ENABLED=Yes
TEST_ACCESS_ANALYZER_ENABLED=Yes
TEST_DETECTIVE_ENABLED=No
TEST_INSPECTOR_ENABLED=No